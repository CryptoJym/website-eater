# Google API Key for Gemini
# Get your key from: https://console.cloud.google.com/apis/credentials
GOOGLE_API_KEY=your-api-key-here

# OpenAI API Key (optional - for embeddings if using mem0)
OPENAI_API_KEY=optional-openai-key

# Mem0 API Key (optional - if using cloud version)
MEM0_API_KEY=optional-mem0-key

# Gemini Model to use
# Available models: gemini-2.5-pro-preview-03-25 (recommended for higher quota)
GEMINI_MODEL=gemini-2.5-pro-preview-03-25

# User and Agent IDs
USER_ID=default_user
AGENT_ID=website_eater_agent

# Flask configuration
FLASK_ENV=development
FLASK_DEBUG=True
